{
  "name": "nest-mqtt",
  "version": "1.0.5",
  "description": "A MQTT module for Nest.js",
  "author": "microud,fetaoily",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palmtreefrb/nest-mqtt.git"
  },
  "keywords": [
    "nest-mqtt",
    "nestjs-mqtt",
    "@nestjs/mqtt",
    "mqtt",
    "nestjs"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "publish": "npm run build && npm publish --access=public"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.11 || ^7.0.0 || ^8.0.0 || ^10.0.0",
    "@nestjs/core": "^6.10.11 || ^7.0.0 || ^8.0.0 || ^10.0.0",
    "mqtt": "5.0.5"
  },
  "devDependencies": {
    "@nestjs/common": "10.2.5",
    "@nestjs/core": "10.2.5",
    "@types/mqtt": "2.5.0",
    "@types/node": "20.6.3",
    "prettier": "3.0.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.4",
    "tsconfig-paths": "4.2.0",
    "tslint": "6.1.3",
    "typescript": "5.2.2"
  }
}
